{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "salary": {
        "type": "integer",
        "minimum": 0
      },
      "equity": {
        "type": "number",
        "minimum":0,
        "maximum": 1
      },
      "companyHandle": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      }
    },
    "additionalProperties": false,
    "required": [
      "title",
      "salary",
      "equity",
      "companyHandle"
    ]
  }
  